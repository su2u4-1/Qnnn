/*test class, type*/

class list<T>(arr) {
    attr arr<T> c;
    attr int size;
    method public void _init(self, arr<T> a) {
        if (a == null) {
            Error("Null array passed to list constructor");
        }
        if (a.size() <= 0) {
            self.size = 4;
        } else {
            self.size = a.size();
        }
        self.c = arr<T>(self.size);
        for (int i = 0; i < a.size(); i++) {
            self.c[i] = a;
        }
        return;
    }

    method public void _init(self, T a, int size) {
        if (size <= 0) {
            Error("Size must be greater than 0");
        }
        self.c = arr<T>(size);
        self.size = size;
        for (int i = 0; i < size; i++) {
            self.c[i] = a;
        }
        return;
    }

    method public int size(self) {
        return self.size;
    }

    method public T _get(self, int i) {
        if (i < 0 || i >= self.size) {
            Error("Index out of range");
        }
        return self.c[i];
    }

    method public void _set(self, int i, T a) {
        if (i < 0 || i >= self.size) {
            Error("Index out of range");
        }
        self.c[i] = a;
        return;
    }

    method public void append(self, T a) {
        self.size = self.size + 1;
        self.c = arr<T>(self.size);
        for (int i = 0; i < self.size - 1; i++) {
            self.c[i] = self.c[i];
        }
        self.c[self.size - 1] = a;
        return;
    }
}